


MAX = 1000;
def prime[MAX + 5];

for (i = 0; i <= MAX; i = i + 1) {
    prime[i] = 0;    
}

prime[1] = 1;
for (i = 2; i <= MAX; i = i + 1) {
	if (prime[i] == 0) {
        prime[0] = prime[0] + 1;
        prime[prime[0]] = i;
    }
	for (j = 1; j <= prime[0]; j = j + 1) {
		if (prime[j] * i > MAX) {
            break;
        }
		prime[prime[j] * i] = 1;
		if (i % prime[j]) {
            break;
        }
	}
}

print(prime[0]);
print(prime[500]);
